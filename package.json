{
  "name": "@ibm-watson/assistant-simple",
  "description": "Covid-19-Validator",
  "version": "0.1.1",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec babel-node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
